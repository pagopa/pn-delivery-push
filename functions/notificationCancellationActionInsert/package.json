        "eslint": "^8.23.1",
        "eslint-plugin-import": "^2.26.0",
        "lambda-tester": "^4.0.1",
        "mocha": "^9.2.2",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^15.1.0",
        "proxyquire": "^2.1.3",
        "sonarqube-scanner": "^2.8.2"
    },
